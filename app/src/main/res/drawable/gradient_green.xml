<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            >
            <corners
                android:radius="3dp"
                />
            <gradient
                android:startColor="#58a0a3"
                android:endColor="#24ffb7"
                android:type="linear"
                android:angle="45"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle"
            >
            <corners
                android:radius="3dp"
                />
            <gradient
                android:angle="0"
                android:startColor="#00ffffff"
                android:endColor="#5dffffff"
                android:type="linear"
                />
        </shape>
    </item>
</layer-list>
